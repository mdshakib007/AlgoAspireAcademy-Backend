{
    "builds": [
      {
        "src": "config/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.11.3",
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "config/wsgi.py"
      }
    ]
  }
  